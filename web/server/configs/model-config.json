{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "user": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
        "realmRequired": false
    }
  },
  "accessToken": {
    "dataSource": "mongodb",
    "public": false
  },
  "userIdentity": {
    "dataSource": "mongodb",
    "public": true
  },
  "ACL": {
    "dataSource": "mongodb",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongodb",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mongodb",
    "public": false
  },
  "lobby": {
    "dataSource": "mongodb",
    "public": true
  },
  "game": {
    "dataSource": "mongodb",
    "public": true
  },
  "lobbyServer": {
    "dataSource": "lobby-server",
    "public": false
  }
}
